___x_cmd_wx_run(){
    : cd
    x ws run
}

___x_cmd_wx_pkg(){
    local workspace="${1:-.}"
    if [ -n "$2" ]; then
        tar cf "$2" "$workspace" --exclude "$2"
    else
        local name="${PWD##*/}"
        tar cf "$name.wx" "$workspace" --exclude "*.wx"
    fi
}

___x_cmd_wx_unpkg(){
    :
}

___x_cmd_wx_ls(){
    :
}


